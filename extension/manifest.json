{
  "name": "Daniel's Screen Capture Sharing",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "logo.png",
    "128": "smallicon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "externally_connectable": {
    "matches": ["http://localhost/*"]
  },
  "permissions": [
    "desktopCapture",
    "http://localhost/*",
    "tabs"
  ]
}